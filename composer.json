{
    "name": "redcatgirl/yii-bootstrap.386",
    "type": "library",
    "description": "Yii Framework Twitter Bootstrap Extension i386 theme",
    "keywords": [
        "yii",
        "bootstrap",
        "theme",
        "i386",
        "MSDOS"
    ],
    "support": {
        "source": "https://github.com/RedCatGirl/yii-bootstrap.386",
        "issues": "https://github.com/RedCatGirl/yii-bootstrap.386/issues"
    },
    "require": {
        "php": "^7.4|^8.0",
        "kristopolous/bootsrap.386": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "yiisoft/arrays": "^1.0|^2.0",
        "yiisoft/assets": "^2.0",
        "yiisoft/html": "^2.0",
        "yiisoft/json": "^1.0",
        "yiisoft/widget": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.16",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.18",
        "yiisoft/aliases": "^1.1|^2.0",
        "yiisoft/di": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RedCatGirl\\YiiBootstrap386\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedCatGirl\\YiiBootstrap386\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "kristopolous/bootsrap.386",
                "version": "4.0",
                "dist": {
                    "url": "https://github.com/kristopolous/BOOTSTRA.386/blob/master/bootstra.386-latest-v4.zip?raw=true",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
